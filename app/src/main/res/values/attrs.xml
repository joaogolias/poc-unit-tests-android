<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditText">
        <attr name="emptinessIsValid" format="boolean"/>
        <attr name="emptyErrorText" format="string"/>
        <attr name="minLength" format="integer"/>
        <attr name="invalidInputLengthText" format="string"/>
        <attr name="requiredCharacterSet" format="string"/>
        <attr name="missingCharacterErrorText" format="string"/>
        <attr name="hint" format="string"/>
        <attr name="inputType" format="enum">
            <enum name="password" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="SquareInputView">
        <attr name="nextLeftView" format="reference"/>
        <attr name="imeOptions" format="enum">
            <enum name="done" value="0"/>
            <enum name="next" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="CalculationCellView">
        <attr name="operation" format="enum">
            <enum name="sum" value="0"/>
            <enum name="subtraction" value="1"/>
            <enum name="multiplication" value="2"/>
            <enum name="factorial" value="3"/>
        </attr>
    </declare-styleable>

</resources>